{
	"name": "@org.quicko.cliq/sheet-core",
	"version": "1.2.0-beta.0",
	"description": "A library in typescript for common sheet entities of Cliq",
	"author": "Prateik Lohani <prateik.lohani@quicko.com>",
	"main": "dist/index.js",
	"types": "dist/index.d.ts",
	"scripts": {
		"clean": "npm cache clean --force",
		"build-release": "tsc --build && npm ci --omit=dev",
		"prebuild": "rm -rf dist && npm run clean && npm run generate",
		"build": "npm run generate && tsc --build",
		"generate": "npm run generateSchema && npm run generateObjects",
		"generateSchema": "excel-to-schema --input ./resources/excel/ --output ./generated/schemas/",
		"generateObjects": "schema-to-objects --input ./generated/schemas/ --output ./generated/sources"
	},
	"license": "ISC",
	"files": [
		"dist/*/**"
	],
	"exports": {
		".": "./dist/index.js",
		"./Commission/beans": "./dist/generated/sources/Commission/index.js",
		"./Link/beans": "./dist/generated/sources/Link/index.js",
		"./Program/beans": "./dist/generated/sources/Program/index.js",
		"./Promoter/beans": "./dist/generated/sources/Promoter/index.js",
		"./Purchase/beans": "./dist/generated/sources/Purchase/index.js",
		"./SignUp/beans": "./dist/generated/sources/SignUp/index.js"
	},
	"typesVersions": {
		"*": {
			"Commission/beans": [
				"dist/generated/sources/Commission/index.d.ts"
			],
			"Link/beans": [
				"dist/generated/sources/Link/index.d.ts"
			],
			"Program/beans": [
				"dist/generated/sources/Program/index.d.ts"
			],
			"Promoter/beans": [
				"dist/generated/sources/Promoter/index.d.ts"
			],
			"Purchase/beans": [
				"dist/generated/sources/Purchase/index.d.ts"
			],
			"SignUp/beans": [
				"dist/generated/sources/SignUp/index.d.ts"
			]
		}
	},
	"directories": {
		"lib": "./dist"
	},
	"dependencies": {
		"@org.quicko/sheet-core": "^6.0.4",
		"@org.quicko/core": "8.1.0-beta.5",
		"class-transformer": "^0.5.1",
	    "class-validator": "^0.14.2",
    	"reflect-metadata": "^0.2.2"

	},
	"devDependencies": {
		"@org.quicko/sheet-generator": "^6.0.2",
		"@types/node": "^22.7.0",
		"@typescript-eslint/eslint-plugin": "^8.17.0",
		"@typescript-eslint/parser": "^8.17.0",
		"esbuild-plugin-eslint": "^0.3.7",
		"eslint": "^8.2.0",
		"eslint-config-airbnb": "^19.0.4",
		"eslint-config-prettier": "^9.1.0",
		"eslint-import-resolver-typescript": "^3.7.0",
		"typescript": "^5.6.2"
	}
}
