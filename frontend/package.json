		"@ngrx/operators": "^19.0.1",
		"@ngrx/signals": "^19.0.1",
		"@ngrx/store": "^19.0.1",
		"@ngrx/store-devtools": "^19.0.1",
		"@org.quicko/ngx-core": "^2.0.0-RC",
		"@org.quicko/ngx-sheet-core": "^1.0.0-RC",
		"@rxweb/reactive-form-validators": "^13.0.1",
