{
	"name": "frontend",
	"version": "0.0.0",
	"scripts": {
		"ng": "ng",
		"start": "ng serve",
		"build": "ng build",
		"watch": "ng build --watch --configuration development",
		"test": "ng test",
		"clean": "npm cache clean --force",
		"promoter:dev": "ng dev promoter-portal",
		"build:core-lib": "ng build org-quicko-cliq-core",
		"promoter": "ng serve promoter-portal",
		"install-snapshot": "npm --userconfig ~/npmrc/snapshot.npmrc install",
		"generate": "npm run generateSchema && npm run generateObjects",
		"generateSchema": "excel-to-schema --input ./resources/excel/ --output ./projects/org-quicko-cliq-core/generated/schemas",
		"generateObjects": "schema-to-objects --input ./projects/org-quicko-cliq-core/generated/schemas --output ./projects/org-quicko-cliq-core/generated/sources"
	},
	"private": true,
	"dependencies": {
